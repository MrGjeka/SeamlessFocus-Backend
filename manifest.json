{
  "manifest_version": 3,
  "name": "SeamlessFocus",
  "version": "1.0.5",
  "permissions": ["identity", "storage", "tabs"],
  "host_permissions": ["https://*.spotify.com/*"],
  "background": {
    "service_worker": "background.js"
  }, 
  "action": {
    "default_popup": "popup.html"
  }
}